{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native", // Adicione esta linha para permitir JSX
    "target": "esnext", // VocÃª pode escolher es5 ou es6
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}

